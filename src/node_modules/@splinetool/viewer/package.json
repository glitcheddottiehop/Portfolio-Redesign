{
  "name": "@splinetool/viewer",
  "version": "0.9.524",
  "description": "Spline viewer",
  "type": "module",
  "main": "./build/spline-viewer.js",
  "module": "./build/spline-viewer.js",
  "types": "./build/spline-viewer.d.ts",
  "exports": {
    ".": {
      "types": "./build/spline-viewer.d.ts",
      "import": "./build/spline-viewer.js",
      "require": "./build/spline-viewer.cjs"
    }
  },
  "files": [
    "build",
    "README.md"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development node --experimental-json-modules ./esbuild.mjs",
    "build": "cross-env NODE_ENV=production node --experimental-json-modules ./esbuild.mjs && yarn build-types",
    "build-types": "tsc --skipLibCheck --isolatedModules false --noEmit false --emitDeclarationOnly --declaration --outDir build/ --declarationMap false",
    "format": "prettier src/ --write",
    "typecheck": "tsc",
    "example": "cross-env VITE_IS_PROD=true yarn build && vite",
    "test": "yarn test:dev && yarn test:prod",
    "test:dev": "wtr src",
    "test:prod": "MODE=prod wtr src"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "dependencies": {
    "lit": "^2.2.4"
  },
  "devDependencies": {
    "@open-wc/testing": "^3.1.5",
    "@splinetool/runtime": "0.9.524",
    "@web/dev-server-esbuild": "^0.3.3",
    "@web/dev-server-legacy": "^1.0.0",
    "@web/test-runner": "^0.13.27",
    "@web/test-runner-playwright": "^0.8.8",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.16",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "vite": "^4.4.7",
    "vite-plugin-static-copy": "^0.13.0"
  }
}