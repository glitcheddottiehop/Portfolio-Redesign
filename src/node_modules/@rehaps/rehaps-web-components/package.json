{
  "name": "@rehaps/rehaps-web-components",
  "version": "0.21.4",
  "description": "Rehaps web components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/rehaps-web-components/rehaps-web-components.esm.js",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/rehaps/web/Rehaps-web-components.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e --ci --all --coverage",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006 -h localhost --quiet",
    "build-storybook": "storybook build --quiet",
    "stencil-storybook": "concurrently --kill-others \"npm run start\" \"npm run storybook\"",
    "storybook-win": "storybook dev -p 6006 -h localhost --quiet",
    "build-storybook-win": "storybook build --quiet",
    "stencil-storybook-win": "concurrently --kill-others \"npm run start\" \"npm run storybook-win\"",
    "dev": "npm run stencil-storybook-win"
  },
  "dependencies": {
    "jest-junit": "^16.0.0"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage/reports",
    "reportFile": "test-report.xml"
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@material-design-icons/svg": "^0.14.11",
    "@pxtrn/storybook-addon-docs-stencil": "^6.4.1",
    "@stencil/core": "^4.1.0",
    "@stencil/sass": "^3.0.5",
    "@stencil/store": "^2.0.9",
    "@stencil/vue-output-target": "^0.8.6",
    "@storybook/addon-actions": "^7.4.0",
    "@storybook/addon-controls": "^7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/html": "^7.4.0",
    "@storybook/html-webpack5": "^7.4.0",
    "@types/jest": "^27.5.2",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.1",
    "jest": "^27.0.3",
    "jest-cli": "^27.4.5",
    "jest-sonar-reporter": "^2.0.0",
    "lit": "^2.8.0",
    "puppeteer": "^21.1.1",
    "stencil-inline-svg": "^1.1.0",
    "storybook": "^7.4.0",
    "typescript": "^5.2.2"
  },
  "license": "MIT",
  "gitHead": "dd5b27c8348c3eda02e33aeabeed5447ef937d9a"
}
